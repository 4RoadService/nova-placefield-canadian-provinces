{
    "name": "4RoadService/nova-placefield-canadian-provinces",
    "description": "An extended version of the Laravel Nova Place field that supports Canadian provinces",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "place",
        "address"
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "RoadService\\NovaPlacefieldCanadianProvinces\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RoadService\\NovaPlacefieldCanadianProvinces\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
